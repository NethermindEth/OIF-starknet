# =============================================================================
# SOLVER CONFIGURATION
# =============================================================================

# Environment Mode
# FORKING=true (default): Use deployment state addresses (for local forks with fresh deployments)
# FORKING=false: Use .env addresses (for live networks with manually configured addresses)
FORKING=true

# Network Names (set these based on your deployment)
# These names must match the keys in your config.Networks
STARKNET_NETWORK_NAME=Starknet Sepolia
DEFAULT_EVM_DESTINATION=Sepolia

# Live Network Token Addresses (used when FORKING=false)
# Leave empty if using FORKING=true (will use deployment state)
STARKNET_ORCA_ADDRESS=
STARKNET_DOG_ADDRESS=

# Logging
LOG_LEVEL=info
LOG_FORMAT=text

# Solver Behavior
# Global defaults - can be overridden per network below
POLL_INTERVAL_MS=1000
# Number of confirmation blocks to wait before processing (0 for instant, 2+ for safety)
CONFIRMATION_BLOCKS=0
# Starknet-specific confirmation blocks (usually 0 due to faster finality)
STARKNET_CONFIRMATION_BLOCKS=0
# Maximum number of blocks to process in one batch
MAX_BLOCK_RANGE=500

# Gas Settings
MAX_GAS_PRICE_WEI=50000000000
GAS_LIMIT_MULTIPLIER=1.2

